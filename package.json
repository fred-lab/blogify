{
  "name": "blogify",
  "description": "A simple app to write articles and post them on different blogs",
  "author": "Fred Gauthier",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "server:start": "node ./src/bin/server.ts",
    "server:dev": "DEBUG=blogify:* nodemon ./src/bin/server.ts",
    "server:build": "tsc --project .",
    "prod": "npm run server:build && cross-env NODE_ENV=production webpack --mode=production",
    "dev": "cross-env NODE_ENV=development webpack --mode=development",
    "watch": "npm run dev && cross-env NODE_ENV=development webpack serve --mode=development",
    "hot": "cross-env NODE_ENV=development webpack serve --mode=development",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:show": "npm run typeorm -- migration:show",
    "migration:revert": "npm run typeorm -- migration:revert",
    "entity:create": "npm run typeorm -- entity:create",
    "subscriber:create": "npm run typeorm -- subscriber:create"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/react": "^17.0.20",
    "@types/slate": "^0.47.9",
    "@types/slate-react": "^0.22.9",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^6.0.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.2.0",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "express-session": "^1.17.2",
    "morgan": "~1.10.0",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "slate": "^0.65.3",
    "slate-history": "^0.65.3",
    "slate-react": "^0.59.0",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "0.0.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.6",
    "@types/redis": "^2.8.32",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cssnano": "^4.1.11",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.12",
    "postcss-loader": "^4.3.0",
    "sass-loader": "^10.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-manifest-plugin": "^4.0.2"
  }
}
